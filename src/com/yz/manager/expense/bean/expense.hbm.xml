<?xml version="1.0" encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.yz.manager.expense.bean.expense" table="expense">
    
        <id name="id" column="id" type="int">
        	<generator class="native" />
        </id>
        <property name="version" type="int"/> 
        <property name="userName" /> 
        <property name="department" /> 
        <property name="firstCName" /> 
        <property name="secondCName" /> 
         <property name="addDate" type="java.sql.Timestamp" /> 
        <property name="content" />
        <property name="supplier"/>
        <property name="supplierAddress"/>
        <property name="contactName"  /> 
        <property name="contactPhone"/> 
         <property name="unitPrice"/>
           <property name="number"/>
        <property name="TotalPrice"/>  
        <property name="company"/>
	    <property name="remarks"/>  
	    <property name="payMode"/>
        <property name="everify"/>
         <property name="nature"/>
          <property name="everifyName"/>
          <property name="unit"/>
    </class>

</hibernate-mapping>
