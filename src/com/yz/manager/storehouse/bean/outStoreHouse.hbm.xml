<?xml version="1.0" encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.yz.manager.storehouse.bean.outStoreHouse" table="outstorehouse">
    
        <id name="id" column="id" type="int">
        	<generator class="native" />
        </id>
        <property name="version" type="int"/> 
        <property name="userName" /> 
        <property name="department" /> 
        <property name="firstCName" /> 
        <property name="secondCName" /> 
        <property name="houseId" /> 
        <property name="outDate" type="java.sql.Timestamp" /> 
        <property name="applyDate" type="java.sql.Timestamp" /> 
        <property name="applyDepartment" />
        <property name="purpose"/>
        <property name="houseVerifyName"/>
        <property name="unit"  /> 
        <property name="outRemarks"/> 
        <property name="outVerify"/>
        <property name="inVerifyName"/>
        <property name="houseManager"/>
        <property name="applyCount"/>  
         <property name="inVerifyRemarks"/>
        <property name="houseVerifyRemarks"/>
        <property name="houseManagerRemarks"/>  
         <property name="nextVerifyName"/>  
    </class>

</hibernate-mapping>
