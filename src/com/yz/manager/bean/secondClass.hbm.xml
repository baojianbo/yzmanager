<?xml version="1.0" encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.yz.manager.bean.secondClass" table="secondclass">
    
        <id name="id" column="id" type="int">
        	<generator class="native" />
        </id>
        <property name="department"/> 
        <property name="systemName"/> 
        <property name="firstCName" />
        <property name="secondCName" />
         <property name="inCount"/> 
        <property name="outCount"/> 
        <property name="unitPrice" />
        <property name="unit" />
          <property name="currentCount" />
	     <property name="houseId" />
    </class>

</hibernate-mapping>
