<?xml version="1.0" encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.yz.manager.archives.bean.archives" table="archives">
    
        <id name="id" column="id" type="int">
        	<generator class="native" />
        </id>
        <property name="version" type="int"/> 
        <property name="userName" /> 
        <property name="department" /> 
        <property name="firstCName" /> 
        <property name="secondCName" /> 
         <property name="arDate" type="java.sql.Timestamp" /> 
        <property name="fileName" />
        <property name="fileNumber"/>
        <property name="filePages"  /> 
        <property name="giveArName"/> 
         <property name="saveArDepartment"/>
        <property name="saveArName"/>
          <property name="saveArDate" type="java.sql.Timestamp" /> 
	    <property name="fileCoverNumber"  />
        <property name="saveYears"/>
	    <property name="remarks"/>
	    <property name="saveArRemarks"/>
        <property name="averify"/>
         <property name="fileFileName"/>
          <property name="fileDir"/>
    </class>

</hibernate-mapping>
